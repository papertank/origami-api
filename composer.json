{
    "name": "origami/api",
    "description": "API Laravel 5 package",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "david@papertank.com"
        }
    ],
    "require": {
        "php": ">=7.1.8",
        "illuminate/support": "~5.5|^6.0|^7.0|^8.0",
        "illuminate/contracts": "~5.5|^6.0|^7.0|^8.0",
        "illuminate/http": "~5.5|^6.0|^7.0|^8.0",
        "league/fractal": "0.19.*",
        "nesbot/carbon": "^1.26.3 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Api\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7-dev"
        },
        "laravel": {
            "providers": [
                "Origami\\Api\\ApiServiceProvider"
            ],
            "aliases": {
                "Api": "Origami\\Api\\ApiFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
