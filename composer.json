{
    "name": "origami/api",
    "description": "API Laravel 5 package",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "david@papertank.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "league/fractal": "^0.19",
        "nesbot/carbon": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Api\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Origami\\Api\\ApiServiceProvider"
            ],
            "aliases": {
                "Api": "Origami\\Api\\ApiFacade"
            }
        }
    },
    "minimum-stability": "stable"
}
